<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pparemusnoc">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="com.example.submissionwahyu.READ_DATABASE"/>
    <uses-permission android:name="com.example.submissionwahyu.WRITE_DATABASE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/githubicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/githubicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SubmissionWahyu">
        <activity android:name=".view.MainActivity" />
        <activity android:name=".view.SplashscreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <queries>
        <package android:name="com.example.submissionwahyu"/>
    </queries>

</manifest>